{
  "compilerOptions": {
    "target": "es6",                                  
    "module": "commonjs",                                
    "rootDir": "./src",                                  
    "baseUrl": "./src",                                  
    "typeRoots": ["node_modules/@types", "typings/app.d.ts"],                                 
    "sourceMap": true,                                
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                     
    "noImplicitAny": true,                            
    "strictNullChecks": true,                         
    "noUnusedLocals": true,                          
    "noUnusedParameters": true, 
    "skipLibCheck": true 
  }
}
